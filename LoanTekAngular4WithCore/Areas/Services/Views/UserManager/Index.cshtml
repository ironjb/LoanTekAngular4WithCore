@{
	ViewBag.Title = "User Manager";
}
<div class="container">
	<div class="lt-breadcrumb">
		<ul id="LTClientSiteBreadcrumb" class="breadcrumb">
			<li class="breadcrumb-item"><i class="fa fa-home"></i><a href="/Services">Services Home</a></li>
			<li class="breadcrumb-item">User Manager</li>
		</ul>
	</div>
</div>
<div class="container pb-4">
	<sum-app>
		<i class="fa fa-circle-o-notch fa-spin fa-fw"></i> Loading Scripts...
	</sum-app>
</div>

@section head {
	<base href="/">
}

@section scripts {
	<environment include="Development">
		<script type="text/javascript" src="/js/Services/UserManager/tmp/webpack/vendor-polyfills.js"></script>
		<script type="text/javascript" src="/js/Services/UserManager/tmp/webpack/app.js"></script>
	</environment>
	<environment exclude="Development">
		<script type="text/javascript" src="/js/lib/core-js/shim.min.js"></script>
		<script type="text/javascript" src="/js/lib/zonejs/zone.min.js"></script>
		<script type="text/javascript" src="/js/Services/UserManager/build/sum-build.js"></script>
	</environment>
}
